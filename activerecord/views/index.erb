<!DOCTYPE html>
<html>
	<head>
	</head>
	<body>
		<p>How many presidents do we have? <%= params[:presidents].length %></p>
		<% 1.upto(44) do |n| %>
			<span style="color:rgb(<%=rand(255)%>,<%=rand(255)%>,<%=rand(255)%>);">
				<%= n %>
			</span>
		<% end %>

		<ul>
			<% params[:presidents].each do |p| %>
				<li style="color:rgb(<%=rand(255)%>,<%=rand(255)%>,<%=rand(255)%>);">
					<%= p.name %>
				</li>
			<% end %>
		</ul>

		<p>Hello, nurse.</p>
	</body>
	</html>